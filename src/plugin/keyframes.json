{
    "swing": {
        "default": {
            "transform": "scale(1) rotate3d(0, 1, 0, 0deg) translateZ(0) translateX(0rem)"
        },
        "rightIn": {
            "transform": "scale(0.8) rotate3d(0, 1, 0, 90deg) translateZ(0) translateX(-2rem)"
        },
        "rightOut": {
            "transform": "scale(0.8) rotate3d(0, 1, 0, -90deg) translateZ(0) translateX(-2rem)"
        },
        "leftIn": {
            "transform": "scale(0.8) rotate3d(0, 1, 0, 90deg) translateZ(0) translateX(-2rem)"
        },
        "leftOut": {
            "transform": "scale(0.8) rotate3d(0, 1, 0, -90deg) translateZ(0) translateX(-2rem)"
        }
    },
    "spin": {
        "default": {
            "transform": "scale(1) rotate3d(1, 0, 0, 0deg) translateZ(0) translateX(0rem)"
        },
        "rightIn": {
            "transform": "scale(0.8) rotate3d(1, 0, 0, 90deg) translateZ(0) translateX(-2rem)"
        },
        "rightOut": {
            "transform": "scale(0.8) rotate3d(1, 0, 0, -90deg) translateZ(0) translateX(-2rem)"
        },
        "leftIn": {
            "transform": "scale(0.8) rotate3d(1, 0, 0, 90deg) translateZ(0) translateX(-2rem)"
        },
        "leftOut": {
            "transform": "scale(0.8) rotate3d(1, 0, 0, -90deg) translateZ(0) translateX(-2rem)"
        }
    },
    "slide": {
        "default": {
            "transform": "scale(1) translateX(0) translateZ(0)"
        },
        "rightIn": {
            "transform": "scale(0.9) translateX(5rem) translateZ(0)"
        },
        "rightOut": {
            "transform": "scale(0.9) translateX(-5rem) translateZ(0)"
        },
        "leftIn": {
            "transform": "scale(0.9) translateX(-5rem) translateZ(0)"
        },
        "leftOut": {
            "transform": "scale(0.9) translateX(5rem) translateZ(0)"
        }
    },
    "fade": {
        "default": {
            "transform": "scale(1) translateZ(0)"
        },
        "rightIn": {
            "transform": "scale(0.98) translateZ(0)"
        },
        "rightOut": {
            "transform": "scale(1.02) translateZ(0)"
        },
        "leftIn": {
            "transform": "scale(0.98) translateZ(0)"
        },
        "leftOut": {
            "transform": "scale(1.02) translateZ(0)"
        }
    },
    "swipe": {
        "default": {
            "transform": "scale(1) translateX(0) translateZ(0)"
        },
        "rightIn": {
            "transform": "scale(1.2) translateX(10rem) translateZ(0)"
        },
        "rightOut": {
            "transform": "scale(0.9) translateX(-10rem) translateZ(0)"
        },
        "leftIn": {
            "transform": "scale(1.2) translateX(-10rem) translateZ(0)"
        },
        "leftOut": {
            "transform": "scale(0.9) translateX(10rem) translateZ(0)"
        }
    },
    "rotate": {
        "default": {
            "transform": "scale(1) rotate3d(1, 1, 0, 0deg) translateZ(0)"
        },
        "rightIn": {
            "transform": "scale(0.8) rotate3d(1, 1, 0, -25deg) translateZ(0)"
        },
        "rightOut": {
            "transform": "scale(1.2) rotate3d(1, 1, 0, 25deg) translateZ(0)"
        },
        "leftIn": {
            "transform": "scale(0.8) rotate3d(1, 1, 0, 25deg) translateZ(0)"
        },
        "leftOut": {
            "transform": "scale(1.2) rotate3d(1, 1, 0, -25deg) translateZ(0)"
        }
    },
    "snake": {
        "origin": {
            "rightIn": "right center",
            "rightOut": "left center",
            "leftIn": "left center",
            "leftOut": "right center"
        },
        "default": {
            "transform": "scaleX(1) translateZ(0)"
        },
        "rightIn": {
            "transform": "scaleX(0.6) translateZ(0)"
        },
        "rightOut": {
            "transform": "scaleX(0.6) translateZ(0)"
        },
        "leftIn": {
            "transform": "scaleX(0.6) translateZ(0)"
        },
        "leftOut": {
            "transform": "scaleX(0.6) translateZ(0)"
        }
    },
    "scroll": {
        "origin": {
            "rightIn": "right center",
            "rightOut": "left center",
            "leftIn": "left center",
            "leftOut": "right center"
        },
        "default": {
            "transform": "translateX(0) translateZ(0)"
        },
        "rightIn": {
            "transform": "translateX(20vw) translateZ(0)"
        },
        "rightOut": {
            "transform": "translateX(-20vw) translateZ(0)"
        },
        "leftIn": {
            "transform": "translateX(-20vw) translateZ(0)"
        },
        "leftOut": {
            "transform": "translateX(20vw) translateZ(0)"
        }
    },
    "window": {
        "origin": {
            "rightIn": "right center",
            "rightOut": "left center",
            "leftIn": "left center",
            "leftOut": "right center"
        },
        "default": {
            "transform": "scale(1) rotate3d(0, 1, 0, 0deg) translateZ(0)"
        },
        "rightIn": {
            "transform": "scale(0.9) rotate3d(0, 1, 0, -90deg) translateZ(0)"
        },
        "rightOut": {
            "transform": "scale(0.9) rotate3d(0, 1, 0, 90deg) translateZ(0)"
        },
        "leftIn": {
            "transform": "scale(0.9) rotate3d(0, 1, 0, 90deg) translateZ(0)"
        },
        "leftOut": {
            "transform": "scale(0.9) rotate3d(0, 1, 0, -90deg) translateZ(0)"
        }
    },
    "fold": {
        "origin": {
            "rightIn": "top center",
            "rightOut": "top center",
            "leftIn": "top center",
            "leftOut": "top center"
        },
        "default": {
            "transform": "scale(1) rotate3d(1, 0, 0, 0deg) translateZ(0)"
        },
        "rightIn": {
            "transform": "scale(0.95) rotate3d(1, 0, 0, -90deg) translateZ(0)"
        },
        "rightOut": {
            "transform": "scale(0.95) rotate3d(1, 0, 0, -90deg) translateZ(0)"
        },
        "leftIn": {
            "transform": "scale(0.95) rotate3d(1, 0, 0, -90deg) translateZ(0)"
        },
        "leftOut": {
            "transform": "scale(0.95) rotate3d(1, 0, 0, -90deg) translateZ(0)"
        }
    }
}
